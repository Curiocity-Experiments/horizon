# .env.example

# Database
DATABASE_URL="mysql://username:password@localhost:3306/abstracta_feedback"

# Next Auth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here # You can generate this with `openssl rand -base64 32`

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Private Label Configurations
CLIENT1_GOOGLE_CLIENT_ID=client1_google_client_id
CLIENT1_GOOGLE_CLIENT_SECRET=client1_google_client_secret

# Logging
LOG_LEVEL=info

# Optional: For production, you might want to add these
# MYSQL_HOST=your_mysql_host
# MYSQL_PORT=your_mysql_port
# MYSQL_DATABASE=your_mysql_database
# MYSQL_USER=your_mysql_user
# MYSQL_PASSWORD=your_mysql_password
